{
    "manifest_version": 3,
    "name": "nate.tv",
    "description": "Keep track of where you left off in a Twitch VOD",
    "version": "0.1",
    "action": {
      "default_popup": "src/index.html"
    },
    "permissions": [
      "storage"
    ],
    "background": {
      "service_worker": "src/background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.twitch.tv/videos/*"
        ],
        "js": ["src/content.js"]
      }
    ],
    "icons": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }